#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch;
    scanf("%c", &ch);

    if (isalpha(ch)) {
        char lower = tolower(ch);
        if (lower == 'a' || lower == 'e' || lower == 'i' || lower == 'o' || lower == 'u')
            printf("Vowel\n");
        else
            printf("Consonant\n");
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
