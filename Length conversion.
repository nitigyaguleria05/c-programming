#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int cm;
    if (scanf("%d", &cm) != 1) return 0;

    double meters = cm / 100.0;
    double kilometers = cm / 100000.0;

    // Print meters and kilometers space-separated, with 2 decimal places
    printf("%.2f %.2f\n", meters, kilometers);
    return 0;
}
