#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int sumArray(int arr[], int n) {
    if (n == 0) return 0;
    return arr[n - 1] + sumArray(arr, n - 1);
}

int main(void) {
    int n;
    if (scanf("%d", &n) != 1) return 0;
    int arr[100];
    for (int i = 0; i < n; ++i) {
        if (scanf("%d", &arr[i]) != 1) return 0;
    }
    int result = sumArray(arr, n);
    printf("%d\n", result);
    return 0;
}
