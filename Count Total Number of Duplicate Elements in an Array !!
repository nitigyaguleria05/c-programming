#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N], freq[N];

    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
        freq[i] = -1; // Initialize frequency array
    }

    // Count frequency of each element
    for (int i = 0; i < N; i++) {
        if (freq[i] == -1) {
            int count = 1;
            for (int j = i + 1; j < N; j++) {
                if (arr[i] == arr[j]) {
                    count++;
                    freq[j] = 0; // Mark as counted
                }
            }
            freq[i] = count;
        }
    }

    int duplicateCount = 0;
    for (int i = 0; i < N; i++) {
        if (freq[i] > 1) {
            duplicateCount++;
        }
    }

    printf("%d\n", duplicateCount);
    return 0;
}
