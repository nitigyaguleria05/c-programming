#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int is_armstrong(int n) {
    if (n < 0) return 0;
    int orig = n;
    int digits = 0;
    int temp = n;
    if (temp == 0) digits = 1;
    while (temp > 0) { digits++; temp /= 10; }

    int sum = 0;
    temp = n;
    while (temp > 0) {
        int d = temp % 10;
        int p = 1;
        for (int i = 0; i < digits; ++i) p *= d;
        sum += p;
        temp /= 10;
    }
    return sum == orig;
}

int is_prime(int n) {
    if (n <= 1) return 0;
    if (n == 2) return 1;
    if (n % 2 == 0) return 0;
    int lim = (int)sqrt((double)n);
    for (int i = 3; i <= lim; i += 2)
        if (n % i == 0) return 0;
    return 1;
}

int is_perfect(int n) {
    if (n <= 1) return 0;
    int sum = 1;
    int lim = (int)sqrt((double)n);
    for (int i = 2; i <= lim; ++i) {
        if (n % i == 0) {
            sum += i;
            int other = n / i;
            if (other != i) sum += other;
        }
    }
    return sum == n;
}

int main(void) {
    int n;
    if (scanf("%d", &n) != 1) return 0;

    if (is_armstrong(n)) {
        printf("Armstrong Number\n");
    } else if (is_prime(n)) {
        printf("Prime Number\n");
    } else if (is_perfect(n)) {
        printf("Perfect Number\n");
    } else {
        printf("None of these\n");
    }
    return 0;
}
