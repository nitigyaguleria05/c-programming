#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int R, C;
    scanf("%d %d", &R, &C);

    int arr[R][C];
    int colSum[C];

    
    for (int j = 0; j < C; j++) {
        colSum[j] = 0;
    }

    
    for (int i = 0; i < R; i++) {
        int rowSum = 0;
        for (int j = 0; j < C; j++) {
            scanf("%d", &arr[i][j]);
            rowSum += arr[i][j];
            colSum[j] += arr[i][j];
        }
        printf("%d\n", rowSum);
    }

    
    for (int j = 0; j < C; j++) {
        printf("%d\n", colSum[j]);
    }

    return 0;
}
