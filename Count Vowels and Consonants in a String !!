#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char S[1001];
    int vowels = 0, consonants = 0;

    fgets(S, sizeof(S), stdin);

    for (int i = 0; S[i] != '\0'; i++) {
        if (isalpha(S[i])) {
            char ch = tolower(S[i]);
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')
                vowels++;
            else
                consonants++;
        }
    }

    printf("%d %d\n", vowels, consonants);

    return 0;
}
