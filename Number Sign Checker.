#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;

    // Read input
    scanf("%d", &N);

    // Check and classify
    if (N > 0) {
        printf("Positive\n");
    } else if (N < 0) {
        printf("Negative\n");
    } else {
        printf("Zero\n");
    }

    return 0;
}
