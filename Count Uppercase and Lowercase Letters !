#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char S[1001];
    int upper = 0, lower = 0;

    fgets(S, sizeof(S), stdin);

    for (int i = 0; S[i] != '\0'; i++) {
        if (isupper(S[i]))
            upper++;
        else if (islower(S[i]))
            lower++;
    }

    printf("%d %d\n", upper, lower);

    return 0;
}
