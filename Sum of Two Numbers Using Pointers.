#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    long long a, b;           
    if (scanf("%lld %lld", &a, &b) != 2) return 0;

    long long *pa = &a;       
    long long *pb = &b;       

    long long sum = *pa + *pb; 

    printf("%lld\n", sum);
    return 0;
}   
   
