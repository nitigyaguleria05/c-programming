#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int max = arr[0], second_max = -10001;
    for (int i = 1; i < N; i++) {
        if (arr[i] > max) {
            second_max = max;
            max = arr[i];
        } else if (arr[i] < max && arr[i] > second_max) {
            second_max = arr[i];
        }
    }

    if (second_max == -10001) {
        printf("No second largest element\n");
    } else {
        printf("%d\n", second_max);
    }

    return 0;
}
