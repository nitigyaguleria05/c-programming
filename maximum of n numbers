#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N, num, max;

    scanf("%d", &N);

    scanf("%d", &max);

    for (int i = 1; i < N; i++) {
        scanf("%d", &num);
        if (num > max) {
            max = num;
        }
    }

    printf("%d\n", max);
    return 0;
}
