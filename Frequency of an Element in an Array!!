#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N, key, count = 0;
    scanf("%d", &N); 

    int arr[N];     

    
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    
    scanf("%d", &key);

    
    for (int i = 0; i < N; i++) {
        if (arr[i] == key) {
            count++;
        }
    }

    
    printf("%d\n", count);

    return 0;
}
