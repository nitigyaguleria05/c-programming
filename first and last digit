#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    long long N;
    if (scanf("%lld", &N) != 1) return 0;

    unsigned long long x = (N < 0)
        ? (unsigned long long)(-(N + 1)) + 1
        : (unsigned long long)N;

    if (x == 0) {
        printf("0 0\n");
        return 0;
    }

    unsigned int last = x % 10;

    while (x >= 10) x /= 10;
    unsigned int first = (unsigned int)x;

    printf("%u %u\n", first, last);
    return 0;
}
