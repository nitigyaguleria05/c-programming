#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

unsigned long long factorial(int num) {
    if (num <= 1) return 1ULL;
    return (unsigned long long)num * factorial(num - 1);
}

int main(void) {
    int num;
    if (scanf("%d", &num) != 1) return 0;
    if (num < 0) return 0; /* constraint ensures num >= 0 */
    unsigned long long result = factorial(num);
    printf("%llu\n", result);
    return 0;
}
