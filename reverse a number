#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    long long N;
    if (scanf("%lld", &N) != 1) return 0;

        unsigned long long x = (N < 0)
        ? (unsigned long long)(-(N + 1)) + 1
        : (unsigned long long)N;

        unsigned long long rev = 0;
    while (x > 0) {
        rev = rev * 10 + (x % 10);
        x /= 10;
    }

        if (N == 0) {
        printf("0\n");
        return 0;
    }

    if (N < 0) putchar('-');
    printf("%llu\n", rev);
    return 0;
}

