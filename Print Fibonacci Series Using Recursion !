#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int fib(int k) {
    if (k <= 0) return 0;
    if (k == 1) return 1;
    return fib(k - 1) + fib(k - 2);
}

void printFib(int n) {
    if (n <= 0) return;
    if (n == 1) {
        printf("0");
        return;
    }
    if (n == 2) {
        printf("0 1");
        return;
    }
    printFib(n - 1);
    printf(" %d", fib(n - 1));
}

int main(void) {
    int n;
    if (scanf("%d", &n) != 1) return 0;
    if (n <= 0) {
        printf("\n");
        return 0;
    }
    printFib(n);
    printf("\n");
    return 0;
}
