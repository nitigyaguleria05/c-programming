#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    if (scanf("%d", &N) != 1) return 0;
    if (N < 0) { printf("0\n"); return 0; }

    int capacity = 5000;
    int *digits = calloc(capacity, sizeof(int));
    if (!digits) return 0;
    digits[0] = 1;
    int size = 1;

    for (int x = 2; x <= N; ++x) {
        int carry = 0;
        for (int i = 0; i < size; ++i) {
            long long prod = (long long)digits[i] * x + carry;
            digits[i] = prod % 10;
            carry = prod / 10;
        }
        while (carry) {
            digits[size++] = carry % 10;
            carry /= 10;
            if (size >= capacity) {
                int newcap = capacity * 2;
                int *tmp = realloc(digits, newcap * sizeof(int));
                if (!tmp) { free(digits); return 0; }
                for (int k = capacity; k < newcap; ++k) tmp[k] = 0;
                digits = tmp;
                capacity = newcap;
            }
        }
    }

    for (int i = size - 1; i >= 0; --i) putchar('0' + digits[i]);
    putchar('\n');

    free(digits);
    return 0;
}
