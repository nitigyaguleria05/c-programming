#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    double celsius;
    if (scanf("%lf", &celsius) != 1) return 0;

    double fahrenheit = (celsius * 9.0 / 5.0) + 32.0;

    // Output up to 2 decimal places
    printf("%.2f\n", fahrenheit);
    return 0;
}
